<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
<bean id="question" class="com.poo.assign2.Question">
<property name="questionId" value="1"/>
<property name="question" value="whats ur name:"/>
<property name="answer" value="poojasree"/>
</bean>
<bean id="question1" class="com.poo.assign2.Question">
<property name="questionId" value="2"/>
<property name="question" value="what u doing:"/>
<property name="answer" value="fine"/>
</bean>
<bean id="list" class="com.poo.assign2.List">
<property name="answers">
<set>
<ref bean="question"></ref>
<ref bean="question1"></ref>
</set> 
</property>
</bean>
</beans>